{
    "name": "aztech/event-bus",
    "description": "Event bus library - core only",
    "keywords": [
        "event",
        "events",
        "dispatch",
        "aztech",
        "bus",
        "event-bus"
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Aztech\\Events\\Bus\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "aztech/event-dispatcher": "~1",
        "aztech/php-utils": "~0",
        "psr/log": "~1.0",
        "rhumsaa/uuid": "~2",
        "ocramius/instantiator": "~1",
        "centraldesktop/protobuf-php": "~0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "squizlabs/php_codesniffer": "~1.5",
        "vektah/bugfree-dangerzone": "0.2.0",
        "phpmd/phpmd": "~2.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
